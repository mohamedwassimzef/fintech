<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.SeparatorMenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="810.0" prefWidth="818.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="File">
          <items>
            <MenuItem mnemonicParsing="false" text="New" />
            <MenuItem mnemonicParsing="false" text="Open…" />
            <Menu mnemonicParsing="false" text="Open Recent" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Close" />
            <MenuItem mnemonicParsing="false" text="Save" />
            <MenuItem mnemonicParsing="false" text="Save As…" />
            <MenuItem mnemonicParsing="false" text="Revert" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Preferences…" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Quit" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
          <items>
            <MenuItem mnemonicParsing="false" text="Undo" />
            <MenuItem mnemonicParsing="false" text="Redo" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Cut" />
            <MenuItem mnemonicParsing="false" text="Copy" />
            <MenuItem mnemonicParsing="false" text="Paste" />
            <MenuItem mnemonicParsing="false" text="Delete" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem mnemonicParsing="false" text="Select All" />
            <MenuItem mnemonicParsing="false" text="Unselect All" />
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TabPane fx:id="mainTabs" prefHeight="785.0" prefWidth="847.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab fx:id="assetFormTab" text="Untitled Tab 1">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="560.0" prefWidth="491.0">
                           <children>
                              <VBox layoutX="64.0" layoutY="124.0" prefHeight="276.0" prefWidth="271.0">
                                 <children>
                                    <Label text="name" />
                                    <TextField fx:id="AssName" />
                                    <Label text="Type" />
                                    <TextField fx:id="AssType" />
                                    <Label text="Value" />
                                    <TextField fx:id="AssValue" />
                                    <Label text="Description" />
                                    <TextField fx:id="AssDesc" />
                                    <Label text="Created at " />
                                    <DatePicker fx:id="AssCreated" prefHeight="25.0" prefWidth="269.0" />
                                    <Label text="User ID" />
                                    <TextField fx:id="AssUser" />
                                    <Button fx:id="SubAsset" mnemonicParsing="false" onAction="#handleSubmitButton" text="Submit" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab fx:id="assetListTab" text="Asset List">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TableView fx:id="assetsTable" layoutX="86.0" layoutY="57.0" prefHeight="398.0" prefWidth="675.0">
                                <columns>
                                  <TableColumn fx:id="colId" prefWidth="60.0" text="ID" visible="false" />
                                  <TableColumn fx:id="colName" prefWidth="120.0" text="Name" />
                                  <TableColumn fx:id="colType" prefWidth="90.0" text="Type" />
                                  <TableColumn fx:id="colValue" prefWidth="90.0" text="Value" />
                                  <TableColumn fx:id="colDescription" prefWidth="150.0" text="Description" />
                                  <TableColumn fx:id="colCreatedAt" prefWidth="140.0" text="Created At" />
                                  <TableColumn fx:id="colUserId" prefWidth="80.0" text="User ID" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                              <Button fx:id="updateAssetButton" layoutX="223.0" layoutY="593.0" mnemonicParsing="false" text="Update" disable="true" onAction="#handleUpdateAssetButton" />
                              <Button fx:id="deleteAssetButton" layoutX="522.0" layoutY="593.0" mnemonicParsing="false" text="Delete" disable="true" onAction="#handleDeleteAssetButton" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab fx:id="contractFormTab" text="Contract Form">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="560.0" prefWidth="491.0">
                           <children>
                              <VBox layoutX="64.0" layoutY="84.0" prefHeight="420.0" prefWidth="320.0">
                                 <children>
                                    <Label text="Contract Number" />
                                    <TextField fx:id="contractNumberField" />
                                    <Label text="Asset ID" />
                                    <TextField fx:id="contractAssetIdField" />
                                    <Label text="User ID" />
                                    <TextField fx:id="contractUserIdField" />
                                    <Label text="Start Date" />
                                    <DatePicker fx:id="contractStartDatePicker" />
                                    <Label text="End Date" />
                                    <DatePicker fx:id="contractEndDatePicker" />
                                    <Label text="Premium Amount" />
                                    <TextField fx:id="contractPremiumField" />
                                    <Label text="Coverage Amount" />
                                    <TextField fx:id="contractCoverageField" />
                                    <Label text="Status" />
                                    <ComboBox fx:id="contractStatusCombo" />
                                    <Label text="Approved By" />
                                    <TextField fx:id="contractApprovedByField" />
                                    <Button fx:id="contractSubmitButton" mnemonicParsing="false" onAction="#handleContractSubmitButton" text="Submit" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab fx:id="contractListTab" text="Contract List">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <TableView fx:id="contractsTable" layoutX="42.0" layoutY="40.0" prefHeight="420.0" prefWidth="760.0">
                                <columns>
                                  <TableColumn fx:id="contractColId" prefWidth="60.0" text="ID" visible="false" />
                                  <TableColumn fx:id="contractColNumber" prefWidth="120.0" text="Number" />
                                  <TableColumn fx:id="contractColAssetId" prefWidth="80.0" text="Asset" />
                                  <TableColumn fx:id="contractColUserId" prefWidth="80.0" text="User" />
                                  <TableColumn fx:id="contractColStart" prefWidth="100.0" text="Start" />
                                  <TableColumn fx:id="contractColEnd" prefWidth="100.0" text="End" />
                                  <TableColumn fx:id="contractColPremium" prefWidth="110.0" text="Premium" />
                                  <TableColumn fx:id="contractColCoverage" prefWidth="120.0" text="Coverage" />
                                  <TableColumn fx:id="contractColStatus" prefWidth="90.0" text="Status" />
                                  <TableColumn fx:id="contractColApprovedBy" prefWidth="90.0" text="Approved" />
                                </columns>
                                 <columnResizePolicy>
                                    <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                                 </columnResizePolicy>
                              </TableView>
                              <Button fx:id="contractUpdateButton" layoutX="230.0" layoutY="590.0" mnemonicParsing="false" text="Update" disable="true" onAction="#handleContractUpdateButton" />
                              <Button fx:id="contractDeleteButton" layoutX="520.0" layoutY="590.0" mnemonicParsing="false" text="Delete" disable="true" onAction="#handleContractDeleteButton" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
      </children>
    </AnchorPane>
  </children>
</VBox>
